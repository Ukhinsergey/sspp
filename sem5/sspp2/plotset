set terminal svg size 600,500
set output 'time.svg'
set title "time"
plot './plot/timeplot.txt' using 1:2 with linespoints title '32ijk', './plot/timeplot.txt' using 1:3 with linespoints title '32ikj', './plot/timeplot.txt' using 1:4 with linespoints title '72ikj'

