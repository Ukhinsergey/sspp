set terminal svg size 600,500
set output 'time.svg'
set title "time"
plot './plot/datplot.txt' using 1:2 with linespoints title '32ijk', './plot/datplot.txt' using 1:3 with linespoints title '32ikj', './plot/datplot.txt' using 1:4 with linespoints title '52ikj'

set output 'ins.svg'
set title "ins"
plot './plot/insplot.txt' using 1:2 with linespoints title '32ijk', './plot/insplot.txt' using 1:3 with linespoints title '32ikj', './plot/insplot.txt' using 1:4 with linespoints title '52ikj'

set output 'l1.svg'
set title "l1"
plot './plot/l1plot.txt' using 1:2 with linespoints title '32ijk', './plot/l1plot.txt' using 1:3 with linespoints title '32ikj', './plot/l1plot.txt' using 1:4 with linespoints title '52ikj'

