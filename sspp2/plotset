set title "l2"
plot './plot/l2plot.txt' using 1:2 with linespoints title '32ijk', './plot/l2plot.txt' using 1:3 with linespoints title '32ikj', './plot/l2plot.txt' using 1:4 with linespoints title '52ikj'

